<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties url="file:C:\Workspaces\private\mytools\MyStarter\generator.properties" />
    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="useMapperCommentGenerator" value="true"/>

        <plugin type="vip.yeee.memo.integrate.custom.generate.core.MapperPlugin">
            <property name="genType" value="${yeee.generator.genType}"/>
            <property name="caseSensitive" value="false"/>
            <property name="forceAnnotation" value="false"/>
            <property name="generateColumnConsts" value="false"/>
            <property name="generateDefaultInstanceMethod" value="false"/>
            <!--<property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>-->
            <!--使用 Data 时，Getter,Setter 无效-->
           <!-- <property name="lombok" value="Getter,Setter,Data,ToString,Accessors,EqualsAndHashCode"/>-->
            <property name="lombok" value="Data"/>
            <property name="swagger" value="false"/>

            <!--使用实际的字段名-->
            <property name="useActualColumnNames" value="true"/>
        </plugin>

        <!--通用代码生成器插件-->

        <!--common____________mapper.xml-->
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.domain.mapper"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/MapperXml.ftl"/>
            <property name="mapperSuffix" value="Mapper"/>
            <property name="fileName" value="${tableClass.shortClassName}${mapperSuffix}.xml"/>
        </plugin>
        <!--common____________Mapper.java-->
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.domain.mapper"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="${yeee.generator.template.path}/Mapper.ftl"/>
            <property name="mapperSuffix" value="Mapper"/>
            <property name="fileName" value="${tableClass.shortClassName}${mapperSuffix}.java"/>
        </plugin>
        <!--common____________Service.java-->
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.service"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="${yeee.generator.template.path}/Service.ftl"/>
            <property name="serviceSuffix" value="Service"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>

        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.vo"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/InfoVo.ftl"/>
            <property name="serviceSuffix" value="InfoVo"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.vo"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/ListVo.ftl"/>
            <property name="serviceSuffix" value="ListVo"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.request"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/IdRequest.ftl"/>
            <property name="serviceSuffix" value="IdRequest"/>
            <property name="fileName" value="${serviceSuffix}.java"/>
        </plugin>
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.request"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/AddRequest.ftl"/>
            <property name="serviceSuffix" value="AddRequest"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.request"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/ListRequest.ftl"/>
            <property name="serviceSuffix" value="ListRequest"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>
        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.model.request"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/model/UpdRequest.ftl"/>
            <property name="serviceSuffix" value="UpdRequest"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>

        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.biz"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/Biz.ftl"/>
            <property name="serviceSuffix" value="Biz"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>

        <plugin type="vip.yeee.memo.integrate.custom.generate.core.TemplateFilePlugin">
            <property name="targetProject" value="${yeee.generator.target.project}"/>
            <property name="targetPackage" value="${yeee.generator.module.parent}.controller"/>
            <property name="modelPackage" value="${yeee.generator.module.parent}"/>
            <property name="templatePath" value="template/common/Controller.ftl"/>
                <property name="serviceSuffix" value="Controller"/>
            <property name="fileName" value="${tableClass.shortClassName}${serviceSuffix}.java"/>
        </plugin>

        <jdbcConnection driverClass="${yeee.generator.datasource.driverClassName}"
                        connectionURL="${yeee.generator.datasource.url}"
                        userId="${yeee.generator.datasource.username}"
                        password="${yeee.generator.datasource.password}">
        </jdbcConnection>

        <!--MyBatis 生成器只需要生成 Model-->
        <javaModelGenerator targetPackage="${yeee.generator.module.parent}.domain.entity" targetProject="${yeee.generator.target.project}"/>
        <table tableName="${yeee.generator.table.name}" domainObjectName="${yeee.generator.table.objectname}">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>
    </context>
</generatorConfiguration>